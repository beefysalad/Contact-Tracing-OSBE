                <i class="fas fa-chart-area fa-fw me-3"></i><span>Profile Settings</span>
              </a>
              <!-- Collapsed content -->
              <ul id="collapseExample2" class="collapse list-group list-group-flush">
                <li class="list-group-item py-2">
                    <a href="/client-profile" class="text-reset">View Profile</a>
                </li>
                <li class="list-group-item py-2">
                    <a href="/client-profile/edit" class="text-reset">Edit Profile</a>
                </li>
                <li class="list-group-item py-2">
                    <a href="/client-new-qr-code" class="text-reset">My QR Code</a>
                </li>
                
              </ul>
              <!-- Collapse 2 -->
            </div>
            
          </div>
         
        </nav>
        <!-- Sidebar -->
      
        <!-- Navbar -->
        <nav id="main-navbar" class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
          <!-- Container wrapper -->
          <div class="container-fluid">
            <!-- Toggle button -->
            <button
              class="navbar-toggler"
              type="button"
              data-mdb-toggle="collapse"
              data-mdb-target="#sidebarMenu"
              aria-controls="sidebarMenu"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <i class="fas fa-bars"></i>
            </button>
      
            <!-- Brand -->
            <a class="navbar-brand" href="#">
                CTT |
              <!-- <img
                src="https://mdbootstrap.com/img/logo/mdb-transaprent-noshadows.png"
                height="25"
                alt=""
                loading="lazy"
              /> -->
            </a>
          
            <!-- Right links -->
            <ul class="navbar-nav ms-auto d-flex flex-row">
              <!-- Notification dropdown -->
              <li class="nav-item dropdown">
                <a
                  class="nav-link me-3 me-lg-0 dropdown-toggle hidden-arrow"
                  href="#"
                  id="navbarDropdownMenuLink"
                  role="button"
                  data-mdb-toggle="dropdown"
                  aria-expanded="false"
                >
                  <i class="fas fa-bell fa-2x"></i>
                  <% if (user.notification.length!==0) { %>
                    <% let badge=0%> 
                    <% for( let index = 0; index < user.notification.length; index++ ) { %>
                        <% if (!user.notification[index].isSeen) { %>
                            <% badge++ %> 
                        <% } %>
                    <% } %>
                    <% if (badge===0) { %>
                        <span class="badge rounded-pill badge-notification bg-danger"></span>
                      <% } else { %>
                      <span class="badge rounded-pill badge-notification bg-danger"><%= badge %> </span>
                      <% } %>
                  <% } %>
                </a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
                    <% if (user.notification.length!==0) { %>
                      <% let badge=0%> 
                      <% for( let index = user.notification.length-1; index >=0; index-- ) { %>
                          <% if (!user.notification[index].isSeen) { %>
                            <% badge++ %> 
                            <li class="notify"><a href="/client/single-notification/<%=user.notification[index]._id%>" class="dropdown-item">Notification from: <%= user.notification[index].header %></a>
                        <% } %>
                       
                      <% } %>
                      <% if (badge===0) { %>
                        <li><a class="dropdown-item">You dont have any notifications</a></li>
                      <% } %>
                    <% } else { %>
                            <li><a class="dropdown-item">You dont have any notifications</a></li>
                    <% } %>
                   
                    <li><a href="/client/notifications/<%=user.notification.length%>" class="dropdown-item text-center notify text-primary">View All</a></li>
                  <!-- <li><a class="dropdown-item" href="#">To do pani diri</a></li>
                  <li><a class="dropdown-item" href="#">To do pani diri</a></li>
                  <li>
                    <a class="dropdown-item" href="#">Topi ngano bakat imoha</a>
                  </li> -->
                </ul>
              </li>
              <!-- Icon -->
              <!-- <li class="nav-item me-3 me-lg-0">
                <a class="nav-link" href="#">
                  <i class="fab fa-github fa-2x"></i>
                </a>
              </li> -->
              <!-- Avatar -->
              <li class="nav-item dropdown">
                <a
                  class="nav-link dropdown-toggle hidden-arrow d-flex align-items-center"
                  href="#"
                  id="navbarDropdownMenuLink"
                  role="button"
                  data-mdb-toggle="dropdown"
                  aria-expanded="false"
                >
                  <img
                    src="<%=user.image%>"
                    class="rounded-circle"
                    height="30"
                    width="30"
                    alt=""
                    loading="lazy"
                  />
                </a>
                <ul
                  class="dropdown-menu dropdown-menu-end"
                  aria-labelledby="navbarDropdownMenuLink"
                >
                  <li class="account-settings"><a class="dropdown-item" href="#"><%=user.firstName%> <%=user.lastName%> </a></li>
                  <li class="account-settings"><a class="dropdown-item" href="/client-map-location">My Location</a></li>
                  <li class="account-settings"><a class="dropdown-item" href="/client-profile">My profile</a></li>
                  <li class="account-settings"><a class="dropdown-item" href="/logoutU">Logout</a></li>
                </ul>
              </li>
            </ul>
          </div>
          <!-- Container wrapper -->
        </nav>
        <!-- Navbar -->
      </header>
      <main style="margin-top: 58px;">
        <ol class="breadcrumb pt-4 px-3">
            <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
            <li class="breadcrumb-item active" aria-current="page">User Logs</li>
          </ol>
        <hr style="margin-top: 5px;">
        <!-- MODAL -->
        <!-- Button trigger modal -->
        <button type="button" id="trigger-modal" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#staticBackdrop" style="display: none;">
            Trigger Modal
        </button>
  
  <!-- Modal -->
        <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" id="modal-target">
            <div class="modal-content">
                <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Establishment Location</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div id="map"></div>
                </div>
                <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                </div>
            </div>
            </div>
        </div>
        <!-- MODAL -->
        <p style="display: none;" class="api_key"><%=map_api_key%> </p>
        <div class="container pt-4">
          <div class="card mt-2">
            <div class="card-header bg-dark text-white">
              User Logs
            </div>
            <div class="card-body">
                <table class="table table-striped table-dark table-hover table-data" id="example">
                    <thead>
                        <tr>
                            <th>Establishment Name</th>
                            <th>Date</th>
                            <th>Time</th>
                            <th>Location</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% if (data) { %>
                            <% for( let index = 0; index < data.userlog.length; index++ ) { %>
                                
                                <tr>
                                    <td><%=data.userlog[index].establishment_name%></td>
                                    <td><%=data.userlog[index].date%></td>
                                    <td><%=data.userlog[index].time%> </td>
                                    <% if (data.userlog[index].location.latitude === '' && data.userlog[index].location.longitude ==='') { %>
                                        <td class="location"><button class="geo"><%=data.userlog[index].address%> </button></td>
                                    <% } else {%>
                                        <td>
                                            <a style="display: none;" href="#" class="target-index"></a>
                                            <button class="geo text-primary" id="geoaddress" onclick="showModal(this)"></button>
                                            <p  style="display: none;" id="latitude"><%=data.userlog[index].location.latitude%></p>
                                            <p  style="display: none;" id="longitude"><%=data.userlog[index].location.longitude%></p>
                                        </td>
                                    <% } %>
                                   
                                    
                                </tr>
                            <% } %>
                        <% } %>
                      
                       
                    </tbody>
                </table>
            
            </div>
          </div>
          
    
        </div>
      </main>
    
