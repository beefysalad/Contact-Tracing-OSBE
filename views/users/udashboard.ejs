            color: white;
            text-decoration: none;
            cursor: pointer;
        }
        div.dataTables_wrapper {
                width: inherit;
                margin: 0 auto;
            }
        @media (max-width:452px) {
            td{
                font-size: 11px;
            }
            th{
                font-size: 11px;
            }
            .main-content{
                padding: 0;
            }
}
    </style>
